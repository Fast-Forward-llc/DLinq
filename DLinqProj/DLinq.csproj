<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net8.0</TargetFrameworks>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<PackageId>DLinq</PackageId>
	<Version>1.0.3</Version>
	<Authors>Phillip Bird</Authors>
	<Company>Fast Forward, LLC</Company>
	<Description>Linq provider for use with Dapper</Description>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<RepositoryUrl>https://github.com/Fast-Forward-llc/DLinq</RepositoryUrl>
	<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	<Title>DLinq</Title>
	<PackageProjectUrl>https://github.com/Fast-Forward-llc/DLinq</PackageProjectUrl>
	<PackageReadmeFile>ReadMe.md</PackageReadmeFile>

  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Dapper" Version="2.1.66" />
    <PackageReference Include="Microsoft.Data.SqlClient" Version="6.1.1" />
    <PackageReference Include="Npgsql" Version="9.0.3" />
  </ItemGroup>
	<ItemGroup>
		<None Include="..\ReadMe.md" Pack="true" PackagePath="\" />
	</ItemGroup>
</Project>
